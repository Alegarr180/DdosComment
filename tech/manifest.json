{
    "manifest_version": 3,
    "name": "Ddos Comment",
    "version": "1.0",
    "description": "Creates comments automatically under the format of Telefonica Tech. /// ctrl+click on button to stay in the curret tab.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      },

      "content_scripts": [
        {
          "js": ["scripts/jquery.js", "scripts/content.js"],
          "matches": [
            "https://antiddos.es.telefonica/page?id=profiled*",
            "https://antiddos.es.telefonica/page?id=host*",
            "https://localhost:4444/page?id=host*",
            "https://localhost:4444/page?id=profiled*",
            "file:///*"
          ]
        }
      ]
  }